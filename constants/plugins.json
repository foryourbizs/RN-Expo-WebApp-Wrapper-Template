{
  "$schema": "./schemas/plugins.schema.json",
  "plugins": {
    "auto": [
      {
        "name": "rnww-plugin-gps",
        "namespace": "gps",
        "method": "registerGpsHandlers"
      },
      {
        "name": "rnww-plugin-background",
        "namespace": "backgr",
        "method": "registerBackgroundHandlers",
        "options": {
          "background": {
            "enableHeadlessBridge": true
          }
        }
      }
    ],
    "manual": [
      {
        "path": "./keep-awake",
        "namespace": "keepaw",
        "method": "registerKeepAwakeHandlers"
      }
    ]
  }
}
